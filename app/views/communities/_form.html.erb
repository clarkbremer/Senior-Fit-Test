<%= render partial: "shared/errors", locals: {object: @community} %>

<%= form_for(community) do |f| %>
  <p>
    <b>Community Name</b><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <b>City</b><br>
    <%= f.text_field :city %>
  </p>
  <p>
    <b>State</b><br>
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
